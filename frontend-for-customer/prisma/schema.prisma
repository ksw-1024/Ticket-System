generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserData {
  id String @id @default(cuid())
  date DateTime @default(now())
  name String
  mail String
  icon String
}

model TikcetData {
  id String @id @default(cuid())
  registrationdate DateTime @default(now())
  name String
  mail String
  title String
  operator String
  description String
  date DateTime
  limitdate DateTime
}

model HaveTicketData {
  id String @id @default(cuid())
  date DateTime @default(now())
  userid String
  ticketid String
}
